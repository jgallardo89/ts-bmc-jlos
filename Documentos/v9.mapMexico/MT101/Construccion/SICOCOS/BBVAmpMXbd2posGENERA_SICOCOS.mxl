<ExplicitRule>//Inicia Codigo BBVA
//string [8] ls_valor;
string [15] ls_operaciones;
string [15] ls_importe;
string [15] ls_importe_iva;
string [250] ls_detalle;
Integer ln_pos_punto;


//Inicializa detalle por registro MT101
	ls_detalle = "";

//Identificador de tipo de registro
	ls_detalle = ls_detalle + "D";

//Entidad o país
	ls_detalle = ls_detalle + "0074";


//Cuenta de servicio
	CONCAT(ls_detalle, FILLER_ZEROS,10-len(trim(#CONTRATOS_CuentaServicio))); //Rellenar con ceros a la izquierda
	ls_detalle = ls_detalle + trim(#CONTRATOS_CuentaServicio);


//Clave de sub-servicio
	CONCAT(ls_detalle, FILLER_ZEROS,9-len(trim(RIGHT(trim(#TIPO_OPERACION_SubServicio),9)))); //Rellenar con ceros a la izquierda
	ls_detalle = ls_detalle + RIGHT(trim(#TIPO_OPERACION_SubServicio),9);


//Descripción de sub-servicio
	ls_detalle = ls_detalle + trim(#TIPO_OPERACION_DescSubservicio);
	CONCAT(ls_detalle, FILLER_SPACES,45-len(trim(#TIPO_OPERACION_DescSubservicio))); //Rellenar con espacios a la derecha

//Periodo de operación (Formato AAAAMM)
	ls_detalle = ls_detalle + mid(gs_fecha_proceso,0,4) + mid(gs_fecha_proceso,5,2);


//Referencia / Número de contrato
	CONCAT(ls_detalle, FILLER_ZEROS,12-len(RIGHT(trim(#CONTRATOS_Referencia),12))); //Rellenar con ceros a la izquierda
	ls_detalle = ls_detalle + RIGHT(trim(#CONTRATOS_Referencia),12);


//Número de operaciones en el mes
	ntoa(#TotalOps, ls_operaciones);
	CONCAT(ls_detalle, FILLER_ZEROS,8-len(ls_operaciones)); //Rellenar con ceros a la izquierda
	ls_detalle = ls_detalle + ls_operaciones;


//Importe acumulado en el mes (rellena con ceros para cliente AA)
	IF #CONTRATOS_IndicadorAAA THEN
	   BEGIN
		IF #CONTRATOS_Moneda = "MXN" THEN
		BEGIN


			ntoa(#imp_comision_mxp,ls_importe);
			ln_pos_punto = strstr(ls_importe,".");


			IF  ln_pos_punto &lt; 0 THEN 
			BEGIN 
				//En caso de que el importe no tenga decimales, Agregar los dos decimales
				ls_operaciones = ls_importe; //Enteros
				ls_operaciones = ls_operaciones + "00";	
	
			END
			ELSE 
			BEGIN
				ls_operaciones = mid(ls_importe,0,ln_pos_punto); //Enteros
				ls_operaciones  = ls_operaciones  + mid(ls_importe, ln_pos_punto+1,2); //Decimales


				IF LEN(mid(ls_importe, ln_pos_punto+1,2)) = 1 THEN
				BEGIN
					ls_operaciones = ls_operaciones + "0";	
				END 
		
			END

		
			CONCAT(ls_detalle , FILLER_ZEROS,15-len(ls_operaciones )); //Rellenar con ceros a la izquierda
			ls_detalle = ls_detalle + ls_operaciones;

		END
		ELSE IF #CONTRATOS_Moneda = "USD" THEN
		BEGIN	


			ntoa(#imp_comision_usd, ls_importe);
	
			ln_pos_punto = strstr(ls_importe,".");


			IF  ln_pos_punto &lt; 0 THEN 
			BEGIN 
				//En caso de que el importe no tenga decimales, Agregar los dos decimales
				ls_operaciones = ls_importe; //Enteros
				ls_operaciones = ls_operaciones + "00";	

			END
			ELSE 
			BEGIN
				ls_operaciones = mid(ls_importe,0,ln_pos_punto); //Enteros
				ls_operaciones  = ls_operaciones  + mid(ls_importe, ln_pos_punto+1,2); //Decimales


				IF LEN(mid(ls_importe, ln_pos_punto+1,2)) = 1 THEN
				BEGIN
					ls_operaciones = ls_operaciones + "0";	
				END
			END


		
		 

			CONCAT(ls_detalle , FILLER_ZEROS,15-len(ls_operaciones )); //Rellenar con ceros a la izquierda
			ls_detalle = ls_detalle + ls_operaciones;

		END
		ELSE 
		BEGIN	

			ntoa(#imp_comision_eur, ls_importe);
		
			ln_pos_punto = strstr(ls_importe,".");


			IF  ln_pos_punto &lt; 0 THEN 
			BEGIN 
				//En caso de que el importe no tenga decimales, Agregar los dos decimales
				ls_operaciones = ls_importe; //Enteros
				ls_operaciones = ls_operaciones + "00";	
	
			END
			ELSE 
			BEGIN
				ls_operaciones = mid(ls_importe,0,ln_pos_punto); //Enteros
				ls_operaciones  = ls_operaciones  + mid(ls_importe, ln_pos_punto+1,2); //Decimales


				IF LEN(mid(ls_importe, ln_pos_punto+1,2)) = 1 THEN
				BEGIN
					ls_operaciones = ls_operaciones + "0";	
				END 

			END
	

		

			CONCAT(ls_detalle , FILLER_ZEROS,15-len(ls_operaciones )); //Rellenar con ceros a la izquierda
			ls_detalle = ls_detalle + ls_operaciones;

		END

	END
	ELSE
 	BEGIN	
		ls_detalle = ls_detalle + FILLER_ZEROS;
	END

//IIndicador de tipo de operación 
	ls_detalle = ls_detalle + "C";

//Indicador de consolidado 
	IF #CONTRATOS_IndicadorAAA  THEN
	   BEGIN
        	ls_detalle = ls_detalle + "N";
	   END
	ELSE
	   BEGIN
        	ls_detalle = ls_detalle + "S";
	   END

//Indicador de cliente AAA
	IF #CONTRATOS_IndicadorAAA THEN
	   BEGIN
