<UsageRelatedFieldName/><ExplicitRule>
<OnBegin>//Inicio Codigo BBVA
string [250] ls_array_escritura_reporte_mt101[99999];
string [250] ls_array_escritura_reporte_mt94X[99999];
//Termina Codigo BBVA</OnBegin>
<OnEnd>//Inicio Codigo BBVA

string [8] ls_operaciones;
string [8] ls_operaciones_mes;
integer gn_SumaOperacionesMes;

integer ln_pos_punto;
integer ln_pos_punto_iva;

string[17] gs_total_comisiones;
string[17] gs_aux_total_comisiones;
string[17] gs_total_iva;
string[17] gs_aux_total_iva;


gn_TotalOperacionesMes = (gn_contador_registros_mt94X-1) + (gn_contador_registros_mt101-1); 
ntoa(gn_TotalOperacionesMes , ls_operaciones);

gn_SumaOperacionesMes = gn_total_operaciones_mt101 + gn_total_operaciones_mt94X;
ntoa(gn_SumaOperacionesMes  , ls_operaciones_mes);

ntoa(gn_total_comisiones,gs_total_comisiones);
ntoa(gn_total_iva,gs_total_iva);



//************************************************************
//HEADER
//************************************************************
//Identificador de tipo de registro
	lg_header_archivo = "H";

//Clave de Servicio
	CONCAT(lg_header_archivo , FILLER_ZEROS,7); //Rellenar con ceros a la izquierda
	lg_header_archivo = lg_header_archivo + "28";

//Fecha Actual Formato (DD-MM-YYYY)
	lg_header_archivo = lg_header_archivo + gs_fecha_proceso;	

//Uso Futuro
	CONCAT(lg_header_archivo , FILLER_SPACES_FUTURO,250-len(lg_header_archivo)); //Rellenar con espacios hacia la derecha

writeblock(lg_header_archivo);




//************************************************************
// SICOCOS AA MT101 
//************************************************************
WHILE gn_contador_escritura_mt101 &lt; gn_contador_registros_mt101 + 1 DO
BEGIN

  writeblock(ls_array_escritura_reporte_mt101[gn_contador_escritura_mt101]);
  gn_contador_escritura_mt101 = gn_contador_escritura_mt101 + 1;

END


//************************************************************
//SICOCOS MT940
//************************************************************
WHILE gn_contador_escritura_mt94X &lt; gn_contador_registros_mt94X + 1 DO
BEGIN

  writeblock(ls_array_escritura_reporte_mt94X[gn_contador_escritura_mt94X]);
  gn_contador_escritura_mt94X = gn_contador_escritura_mt94X + 1;

END



//************************************************************
//
//************************************************************

lg_header_archivo  = "";

//Identificador de tipo de registro
	lg_header_archivo = "P";

//Total de operaciones en el mes
	CONCAT(lg_header_archivo , FILLER_ZEROS,8-len(ls_operaciones_mes)); //Rellenar con ceros a la izquierda
	lg_header_archivo = lg_header_archivo + ls_operaciones_mes;


//Total de importe acumulado en el mes
	lg_header_archivo = lg_header_archivo + FILLER_ZEROS + "00";	

//Total de operaciones Cargo
	CONCAT(lg_header_archivo , FILLER_ZEROS,8-len(ls_operaciones)); //Rellenar con ceros a la izquierda
	lg_header_archivo = lg_header_archivo + ls_operaciones;


//Total de operaciones abono
	CONCAT(lg_header_archivo , FILLER_ZEROS,8); //Rellenar con ceros a la izquierda
 
//Total importe comision 
	ln_pos_punto = strstr(gs_total_comisiones,".");

	IF  ln_pos_punto &lt; 0 THEN 
	BEGIN 
		gs_aux_total_comisiones = gs_total_comisiones; //Enteros
		gs_aux_total_comisiones = gs_aux_total_comisiones + "00";	

	END
	ELSE 
	BEGIN
		gs_aux_total_comisiones = mid(gs_total_comisiones,0,ln_pos_punto); //Enteros
		gs_aux_total_comisiones = gs_aux_total_comisiones + mid(gs_total_comisiones, ln_pos_punto+1,2); //Decimales

		IF LEN(mid(gs_total_comisiones, ln_pos_punto+1,2)) = 1 THEN
		BEGIN
			gs_aux_total_comisiones = gs_aux_total_comisiones + "0";	
		END 
	END



	CONCAT(lg_header_archivo, FILLER_ZEROS,17-len(gs_aux_total_comisiones)); //Rellenar con ceros a la izquierda

	lg_header_archivo = lg_header_archivo + gs_aux_total_comisiones;




//Total de importe de impuesto comision local 1
	ln_pos_punto_iva = strstr(gs_total_iva,".");
	
        IF  ln_pos_punto_iva&lt; 0 THEN 
        BEGIN 
	        gs_aux_total_iva = gs_total_iva; //Enteros
	        gs_aux_total_iva = gs_aux_total_iva + "00";	

        END
        ELSE 
        BEGIN		
	
	        gs_aux_total_iva = mid(gs_total_iva,0,ln_pos_punto_iva); //Enteros
         	gs_aux_total_iva = gs_aux_total_iva + mid(gs_total_iva, ln_pos_punto_iva+1,2); //Decimales

		IF LEN(mid(gs_total_iva, ln_pos_punto_iva+1,2)) = 1 THEN
		BEGIN
			gs_aux_total_iva = gs_aux_total_iva + "0";	
		END 
	

        END


	

        CONCAT(lg_header_archivo, FILLER_ZEROS,17-len(gs_aux_total_iva)); //Rellenar con ceros a la izquierda

        lg_header_archivo = lg_header_archivo + gs_aux_total_iva;



//Total de importe de impuesto comision local 2
	lg_header_archivo = lg_header_archivo + FILLER_ZEROS + "00";

//Total de importe de impuesto comision local 3
	lg_header_archivo = lg_header_archivo + FILLER_ZEROS + "00";

//Total de importe de impuesto comision local 4
	lg_header_archivo = lg_header_archivo + FILLER_ZEROS + "00";

//Uso Futuro
	CONCAT(lg_header_archivo , FILLER_SPACES_FUTURO,250-len(lg_header_archivo)); //Rellenar con espacios hacia la derecha


//Uso Futuro
	CONCAT(lg_header_archivo , FILLER_SPACES_FUTURO,250-len(lg_header_archivo)); //Rellenar con espacios hacia la derecha

writeblock(lg_header_archivo);



//Termina Codigo BBVA</OnEnd>
</ExplicitRule>
