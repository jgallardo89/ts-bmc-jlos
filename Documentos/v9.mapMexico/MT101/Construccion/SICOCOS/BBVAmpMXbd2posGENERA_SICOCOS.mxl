			END 
		END

	
		
		

		CONCAT(ls_detalle , FILLER_ZEROS,9-len(ls_operaciones )); //Rellenar con ceros a la izquierda
		ls_detalle = ls_detalle + ls_operaciones;

	END


//Importe Comisión

	IF #CONTRATOS_IndicadorAAA  THEN
	   BEGIN
        	ls_detalle = ls_detalle + FILLER_ZEROS;
